fn isPrime(n: int) -> bool {
    if n <= 1 { return false; }
    let d: int = 2;
    while d * d <= n {
        if n % d == 0 { return false; }
        d = d + 1;
    }
    return true;
}

fn main() -> int {
    let limit: int = 100000;
    let i: int = 2;
    let count: int = 0;

    while i <= limit {
        if isPrime(i) {
            count = count + 1;
        }
        i = i + 1;
    }

    return count;
}